{
  "projects": {
    "default": "prompt-sharing-f8eeb"
  },
  "firestore": {
    "rules": "config/firestore/firestore.rules"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-functions-debug.log"
      ]
    }
  ],
  "hosting": {
    "site": "prompt-sharing-f8eeb",
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "functions/**"
    ],
    "redirects": [
      { "source": "/favicon.ico", "destination": "/assets/favicon.ico", "type": 301 }
    ]
  }
}
