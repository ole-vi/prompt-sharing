{
  "projects": {
    "default": "prompt-sharing-f8eeb"
  },
  "firestore": {
    "rules": "config/firestore/firestore.rules"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-functions-debug.log"
      ]
    }
  ],
  "hosting": {
    "site": "prompt-sharing-f8eeb",
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "functions/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/pages/home/index.html"
      },
      {
        "source": "/jules",
        "destination": "/pages/jules/jules.html"
      },
      {
        "source": "/profile",
        "destination": "/pages/profile/profile.html"
      },
      {
        "source": "/queue",
        "destination": "/pages/queue/queue.html"
      },
      {
        "source": "/sessions",
        "destination": "/pages/sessions/sessions.html"
      },
      {
        "source": "/webcapture",
        "destination": "/pages/webcapture/webcapture.html"
      }
    ]
  }
}
